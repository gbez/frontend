: RESET_FEED,\n  };\n};\n\n// POST\nexport const loadPost = (queryOverride) => async (dispatch, getState) => {\n  console.log(window.location.pathname);\n  const response = await b3.get(\n    queryOverride ? queryOverride : `/blogpost/${window.location.pathname}`\n  )\n    .then((response) => {\n      dispatch({ type: LOAD_POST, payload: response });\n    })\n    .catch((e) => {\n      console.log(e);\n      toast.error(e.response.data.message);\n    });\n};\n\nexport const resetPost = () => {\n  return {\n    type: RESET_POST,\n  };\n};\n\n// AUTH\nexport const restoreUser = () => async (dispatch) => {\n  const response = await b3.get(\"/user/whoAmI\")\n    .then((response) => {\n      dispatch({ type: RESTORE_USER, payload: response });\n    })\n    .catch((e) => {\n      toast.error(e.response.data.message);\n    });\n};\n\nexport const login = (loginObject) => async (dispatch) => {\n  const response = await b3.post(\"/user/login\", loginObject)\n    .then((response) => {\n      localStorage.setItem(\"token\", response.data.token);\n      dispatch({ type: LOGIN, payload: response.data });\n      toast.success(\"Log In Success\");\n    })\n    .catch((e) => {\n      toast.error(e.response.data.message);\n    });\n};\n\nexport const logout = () => {\n  return {\n    type: LOGOUT,\n  };\n};\n","/Users/gbes/Code/Projects/Web/blog/frontend/src/functions/helpers.js",[],"/Users/gbes/Code/Projects/Web/blog/frontend/src/components/public/Post/ScrollDownArrow.js",[],"/Users/gbes/Code/Projects/Web/blog/frontend/src/components/public/Post/ProgressBar.js",[],"/Users/gbes/Code/Projects/Web/blog/frontend/src/components/public/Feed/FeedItem.js",[],"/Users/gbes/Code/Projects/Web/blog/frontend/src/components/public/Unsubscribe.js",["145","146","147","148","149","150","151"],"import React, { Component } from \"react\";\nimport { connect } from \"react-redux\";\nimport {\n  setRelationship, getRelationship, resetRelationship\n} from \"../../redux/actions\";\nimport { getSafe } from 